<h1>All Questions</h1>
<% @questions.each do |question| %>
<div class="media">
    <div class="media-body">
        <h4 class="media-heading">
          <% conditional_wrapper(question.resolved, tag: :mark) do %>
            <%= link_to question.title, question %>
          <% end %>
        </h4>
    </div>
</div>
<% end %>
